{
  "name": "amelie",
  "version": "1.0.0",
  "description": "HTML5 audio visualiser experiment using D3 with a curious Am√©lie theme.",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "cross-env": "^7.0.3",
    "parcel": "^2.0.0-rc.0"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "react": "^0.14.10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build src/index.html --dist-dir public --no-source-maps",
    "postbuild": "cp -nrv src/media public",
    "prebuild": "rm -fr .parcel-cache public",
    "prestart": "yarn prebuild && mkdir public && yarn postbuild",
    "start": "cross-env NODE_ENV=development parcel src/index.html --dist-dir public --no-autoinstall --no-source-maps --open"
  },
  "alias": {
    "d3": {
      "global": "d3"
    },
    "react": {
      "global": "React"
    }
  }
}
